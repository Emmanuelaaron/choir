<div class="row d-flex flex-column align-items-center">
<div class="col-md-10 my-table pt-4 pb-4">
<div class="section-titl">
          <h2>LUBOWA CHOIR</h2>
</div>

<table class="table table-stripe text-center">
  <h5><u><em><%= link_to 'Download as Excel',
  lubowa_path(format: :xlsx),
  class: 'text-light'
  %></u></em></h5>
  <thead>
    <tr class="text-light">
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Contact</th>
      <th scope="col">voice</th>
      <th scope="col">Cell Number</th>
      <th scope="col">email</th>
      <th scope="col">Descipled?</th>
    </tr>
  </thead>
  <%@members.each do |member|%>
  <tbody class="text-secondary">
    <tr>
      <td ><%=member.firstname%></td>
      <td class="text-light"><%=member.lastname%></td>
      <td class="text-light"><%=member.contact%></td>
      <td class="text-light"><%=member.voice%></td>
      <td class="text-light"><%=member.cellnumber%></th>
      <td class="text-light"><%=member.email%></th>
      <td class="text-light"><%=member.descipled%></th>
    </tr>
  </tbody>
  <%end%>
</table>
</div>
</div>