    <section id="contact" class="contact">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>ADMIN Login In</h2>
        </div>

        <div class="row mt-1 d-flex justify-content-center" data-aos="fade-right" data-aos-delay="100">

          <div class="col-lg-8 mt-5 mt-lg-0" data-aos="fade-left" data-aos-delay="100">

            <%= form_for(:session, url: signin_path, html: {class: "php-email-form"}) do |f| %>
              <div class="row">
                <div class="col-md-6 form-group">
                    <%= f.email_field :email ,class:'form-control', placeholder:'Email', :required => true%>
                </div>
                <div class="col-md-6 form-group mt-3 mt-md-0">
                  <%= f.password_field :password ,class:'form-control', placeholder:'password', :required => true%>
                </div>
              </div>
              <div class="my-3">
                <p class="notice"><%= notice %></p>
                <p class="alert text-danger"><%= alert %></p>
              </div>
              <div class="text-center"><button type="submit">Login</button></div>
              <% end%>
         
          </div>

        </div>

      </div>
    </section>